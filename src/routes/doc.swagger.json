  "paths": {
    "/usuarios/": {
      "post": {
        "tags": [
          "Usuarios"
        ],
        "description": "Endpoint para criar um usuario",
        "parameters": [
          {
            "name": "CriarUsuario",
            "in": "body",
            "description": "Dados do Usuario",
            "required": true,
            "schema": {
              "type": "object",
              "properties": {
                "nome": {
                  "type": "string",
                  "example": "Usuario Teste"
                },
                "data_de_nascimento": {
                  "type": "string",
                  "example": "2000-02-20"
                },
                "cpf": {
                  "type": "string",
                  "example": "111-111-111.11"
                },
                "sexo": {
                  "type": "string",
                  "example": "Masculino / Feminino / Outro"
                },
                "cep": {
                  "type": "string",
                  "example": "44444-444"
                },
                "email": {
                  "type": "string",
                  "example": "email@email.com"
                },
                "password_hash": {
                  "type": "string",
                  "example": "senha"
                }
              },
              "required": [
                "nome",
                "data_de_nascimento",
                "cpf",
                "sexo",
                "cep",
                "email",
                "password_hash"
              ]
            }
          }
        ],
        "responses": {
          "201": {
            "description": "Created"
          },
          "400": {
            "description": "Bad Request"
          },
          "409": {
            "description": "Conflict"
          },
          "500": {
            "description": "Internal Server Error"
          }
        }
      }
    },
